# Global Environment Variables for Docker Compose
# Copy this file to .env and fill in your actual values

# AI Generation Configuration (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration (Default values for development)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres  
POSTGRES_PASSWORD=postgres
POSTGRES_DB=solara

# RabbitMQ Configuration (Default values for development)
RABBITMQ_URL=amqp://rabbitmq:rabbitmq@localhost:5672

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
